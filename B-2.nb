    RowBox[{"Print", "[", 
     RowBox[{"\"\<optimal solution is \>\"", ",", "equl"}], "]"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"secondor", "=", 
     RowBox[{
      RowBox[{"D", "[", 
       RowBox[{"profit", ",", 
        RowBox[{"{", 
         RowBox[{"var", ",", "2"}], "}"}]}], "]"}], "//", "FullSimplify"}]}], 
    ";", "\[IndentingNewLine]", 
    RowBox[{"Print", "[", 
     RowBox[{"\"\<second order derivative is \>\"", ",", "secondor"}], "]"}], 
